package com.company.lifeCycle;

public class Pepsi {



	private int price;

	public int getPrice() {
		return price;
	}

	public void setPrice(int price) {
		this.price = price;
	}

	public Pepsi(int price) {
		super();
		this.price = price;
	}

	public Pepsi() {
		super();
	}
	
	@Override
	public String toString() {
		return "Pepsi [price=" + price + "]";
	}
	
}
